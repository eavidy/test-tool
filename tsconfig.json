{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "lib": ["ESNext"],
    "outDir": "dist",
    "strict": true,
    "skipDefaultLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "allowJs": true,
    "paths": {
      "#root/*": ["./*"]
    }
  },

  "exclude": ["node_modules", "src/bench_ui/assets", "dist/**"]
}
